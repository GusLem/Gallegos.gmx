          <argument>
            <kind>1</kind>
            <string>if instance_exists(sim) {
    sim.alp = alp;
    nao.alp = alp;
}

if !exiting {
    if alp &lt; 1 {
        alp += 0.1
    }
}
else {
    if alp &gt; 0 {
        alp -= 0.1
    }
    else {
        instance_destroy();
    }
}

if leaving {
    if og_room == room {
        if box_alp &lt; 1 {
            box_alp += 0.08;
            global.alpha -= 0.08
        }
        else {
            global.alpha = 0;
            box_alp = 0;
            
            audio_sound_gain(global.musica,0,500);
            audio_sound_gain(global.musica_menu,1,2500);
            
            room_goto(rm_menu)
        }
    }
    else {
        if global.alpha &lt; 1 {
            global.alpha += 0.02
        }
        else {
            instance_destroy();
        }
    }
}



if keyboard_check_pressed(vk_escape) {
    exiting = true;
}
